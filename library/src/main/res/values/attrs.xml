<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextViewCF">
        <attr name="custom_style_font_textview">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
            <enum name="italic_bold" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="EditTextCF">
        <attr name="custom_font_style_edittext">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
            <enum name="italic_bold" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="ButtonCF">
        <attr name="custom_font_style_button">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
            <enum name="italic_bold" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="SpinnerCF">
        <attr name="spinner_item_resource" format="reference" />
        <attr name="spinner_dropdown_item_resource" format="reference" />
    </declare-styleable>
</resources>